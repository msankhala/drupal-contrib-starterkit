{
  "name": "drupal-contrib-starterkit",
  "version": "1.0.0",
  "description": "A npm module to provide pre-commit hook feature.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "validate-branch-name": {
    "pattern": "^(issue-)[0-9]+.+$",
    "errorMsg": "Please use the following branch naming convention: issue-<ISSUE-ID>-some-title, issue-3349080-some-title, issue-3349080-timestampdatetime-widget-not-null"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:msankhala/drupal-contrib-starterkit.git"
  },
  "author": "Mahesh Sankhala (mahesh.saini@materialplus.io)",
  "license": "ISC",
  "devDependencies": {
    "@secretlint/secretlint-rule-preset-recommend": "^6.1.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-jquery": "^3.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-yml": "^1.4.0",
    "husky": "^8.0.3",
    "jira-prepare-commit-msg": "^1.7.2",
    "lint-staged": "^13.1.0",
    "secretlint": "^6.1.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "validate-branch-name": "^1.3.0"
  }
}
